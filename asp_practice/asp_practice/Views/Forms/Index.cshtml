@model IEnumerable<Practice_POSR_MVC.Models.Form>

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Form_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.File_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Display_Seq)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Form_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.File_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Display_Seq)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Form_ID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Form_ID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Form_ID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
